<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee                
	http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	
    <application>
        <el-resolver>
            org.springframework.web.jsf.el.SpringBeanFacesELResolver
        </el-resolver>
    </application>
    
    <lifecycle>
	    <phase-listener id="parfum">com.marcanti.ecommerce.listener.ParfumSecuriteListener</phase-listener>
	</lifecycle>

	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>product</from-outcome>
			<to-view-id>/pages/product.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>change_password</from-outcome>
			<to-view-id>/pages/public/change_password.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>	
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>code</from-outcome>
