			<from-outcome>membre</from-outcome>
			<to-view-id>/pages/private/carousel.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>	
	
	<application>
		<resource-bundle>
			<base-name>ressources.parfumLibelle</base-name>
			<var>msg</var>
		</resource-bundle>
		<locale-config>
			<default-locale>fr</default-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
	</application>
	<application>
		<resource-bundle>
			<base-name>ressources.ApplicationMessage</base-name>
			<var>msgError</var>
		</resource-bundle>
		<locale-config>
			<default-locale>fr</default-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
	</application>
